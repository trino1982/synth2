{
  "name": "synth",
  "version": "1.0.0",
  "description": "Desktop application for synthesizing data from connected apps",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "author": "Chris Meeks",
  "scripts": {
    "start": "concurrently \"npm run proxy\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proxy": "nodemon server/server.js",
    "electron:dev": "concurrently \"npm start\" \"electron .\"",
    "electron:build": "npm run build && electron-builder build --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@slack/web-api": "^6.9.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "firebase": "^10.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31"
  },
  "build": {
    "appId": "com.chrismeeks.synth",
    "productName": "Synth",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
