# OpenAI Configuration
REACT_APP_OPENAI_API_KEY="YOUR_OPENAI_API_KEY"

# Synth App Configuration
REACT_APP_NAME="Synth"
REACT_APP_ENV="development"
PORT=3000
PROXY_PORT=3001
BROWSER=none # Prevents opening browser when running npm start

# Proxy Server Debug Options
DEBUG=true                   # Enable detailed logging
PROXY_REQUEST_TIMEOUT=10000  # Request timeout in milliseconds
PROXY_LOG_LEVEL=debug       # Log level (debug, info, warn, error)
PROXY_MAX_RETRIES=3         # Number of retries for failed requests
